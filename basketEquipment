int yearlyPrice = int.Parse(Console.ReadLine());

double shoesPrice = yearlyPrice * 0.6;
double jerseyPrice = shoesPrice * 0.8;
double ballPrice = jerseyPrice / 4.0;
double accessoriesPrice = ballPrice / 5.0;

double totalSum = yearlyPrice + shoesPrice + jerseyPrice + ballPrice + accessoriesPrice; 

Console.WriteLine(totalSum);
